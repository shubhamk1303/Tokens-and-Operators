#include <iostream>
using namespace std;

int main() 
{
    int x;
    char y;
    float z;
    double a;

    cout<<"Size of int is:\n"<<sizeof(x);
    cout<<"Size of char is:\n"<<sizeof(y);
    cout<<"Size of float is:\n"<<sizeof(z);
    cout<<"Size of double is:\n"<<sizeof(a);


    return 0;
}

